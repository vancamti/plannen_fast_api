[flake8]
application_import_names = app,tests
import-order-style = pycharm

extend-exclude =
    .pytest_cache
    static
    ./oproeppremieaanvragen/common/json_model/openapi.py
filename =
    ./tests/**.py
    ./alembic/**.py

max-line-length = 80
extend-select = B950
extend-ignore = E203,E501,E701,B008
